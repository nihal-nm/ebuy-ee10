<?xml version="1.0" encoding="UTF-8"?><ejb-jar metadata-complete="true" version="3.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
    <display-name>eBuy-ejb</display-name>
    <enterprise-beans>
        <session>
            <ejb-name>Calculator</ejb-name>
            <ejb-class>com.ibm.ebuy.service.Calculator</ejb-class>
            <session-type>Stateless</session-type>
            <concurrency-management-type>Container</concurrency-management-type>
            <local-bean/>
        </session>
        <session>
            <ejb-name>UserBean</ejb-name>
            <business-local>com.ibm.ebuy.service.interfaces.UserBeanLocal</business-local>
            <ejb-class>com.ibm.ebuy.service.UserBean</ejb-class>
            <session-type>Stateless</session-type>
            <concurrency-management-type>Container</concurrency-management-type>
            <local-bean/>
            <persistence-context-ref>
                <persistence-context-ref-name>com.ibm.ebuy.service.UserBean/em</persistence-context-ref-name>
                <persistence-unit-name/>
                <persistence-context-type>Transaction</persistence-context-type>
                <injection-target>
                    <injection-target-class>com.ibm.ebuy.service.UserBean</injection-target-class>
                    <injection-target-name>em</injection-target-name>
                </injection-target>
            </persistence-context-ref>
        </session>
        <session>
            <ejb-name>BookBean</ejb-name>
            <business-local>com.ibm.ebuy.service.interfaces.BookBeanLocal</business-local>
            <ejb-class>com.ibm.ebuy.service.BookBean</ejb-class>
            <session-type>Stateless</session-type>
            <concurrency-management-type>Container</concurrency-management-type>
            <resource-ref>
                <res-ref-name>jdbc/ebuy</res-ref-name>
                <res-type>javax.sql.DataSource</res-type>
                <res-auth>Container</res-auth>
                <injection-target>
                    <injection-target-class>com.ibm.ebuy.service.BookBean</injection-target-class>
                    <injection-target-name>ds</injection-target-name>
                </injection-target>
            </resource-ref>
            <persistence-context-ref>
                <persistence-context-ref-name>com.ibm.ebuy.service.BookBean/entityManager</persistence-context-ref-name>
                <persistence-unit-name>ebuy</persistence-unit-name>
                <persistence-context-type>Transaction</persistence-context-type>
                <injection-target>
                    <injection-target-class>com.ibm.ebuy.service.BookBean</injection-target-class>
                    <injection-target-name>entityManager</injection-target-name>
                </injection-target>
            </persistence-context-ref>
        </session>
        <session>
            <ejb-name>OrderBean</ejb-name>
            <business-local>com.ibm.ebuy.service.interfaces.OrderBeanLocal</business-local>
            <ejb-class>com.ibm.ebuy.service.OrderBean</ejb-class>
            <session-type>Stateless</session-type>
            <concurrency-management-type>Container</concurrency-management-type>
            <local-bean/>
            <persistence-context-ref>
                <persistence-context-ref-name>com.ibm.ebuy.service.OrderBean/entityManager</persistence-context-ref-name>
                <persistence-unit-name>ebuy</persistence-unit-name>
                <persistence-context-type>Transaction</persistence-context-type>
                <injection-target>
                    <injection-target-class>com.ibm.ebuy.service.OrderBean</injection-target-class>
                    <injection-target-name>entityManager</injection-target-name>
                </injection-target>
            </persistence-context-ref>
        </session>
        <session>
            <ejb-name>UserCounterBean</ejb-name>
            <ejb-class>com.ibm.ebuy.service.UserCounterBean</ejb-class>
            <session-type>Stateless</session-type>
            <concurrency-management-type>Container</concurrency-management-type>
            <local-bean/>
        </session>
        <session>
            <ejb-name>CounterBean</ejb-name>
            <ejb-class>com.ibm.ebuy.service.CounterBean</ejb-class>
            <session-type>Singleton</session-type>
            <concurrency-management-type>Container</concurrency-management-type>
            <local-bean/>
        </session>
    </enterprise-beans>
    <assembly-descriptor/>
</ejb-jar>